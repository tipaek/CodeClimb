{
  "name": "codeclimb-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "gen:api": "npx --yes openapi-typescript@7.10.1 ../contracts/openapi.yaml -o src/api/generated/openapi.ts",
    "verify:api": "node scripts/verify-api.mjs",
